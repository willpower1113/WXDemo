/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

  /** 二级界面 公共头部*/
.text-title{
	font-size: 30rpx;
	margin: 80rpx auto;
	display: flex;
  width: 90%;
  justify-content: center;
	text-align: center;
	color: #999999;
}


.flex-row{
  display: flex;
  flex-direction: row;
}

.flex-column{
  display: flex;
  flex-direction: column;
}